{
  "name": "ng2-googlechart",
  "version": "2.0.4",
  "description": "Angular 2 Google Charts library",
  "main": "build/ng2-googlechart.js",
  "typings": "build/ng2-googlechart.d.ts",
  "files": ["build"],
  "scripts": {
    "flow:lint": "tslint src/*.ts",
    "flow.install:typings": "typings install",
