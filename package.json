{
  "name": "ng2-googlechart",
  "version": "2.0.11",
  "description": "Angular 2 Google Charts library",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "ngc -p src",
    "start": "http-server -c-1 -o -p 8875 .",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage-remap": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-remapped.lcov -t lcovonly",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.4.3",
    "@angular/compiler": "^2.0.0",
    "@angular/compiler-cli": "^2.4.3",
    "@angular/core": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.3",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "typescript": "2.1.5",
    "typings": "^2.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "zone.js": "^0.7.5"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shreekantha/ng2-googlechart.git"
  },
  "keywords": [
    "angular2 charts",
    "Charts",
    "chart",
    "charts",
    "google chart",
    "google charts",
    "angular2 google charts",
    "ng charts",
    "ng2 charts",
    "ng2 google charts",
    "ng google charts"
  ],
  "author": "Shreekanth",
  "bugs": {
    "url": "https://github.com/shreekantha/ng2-googlechart/issues"
  },
  "homepage": "https://github.com/shreekantha/ng2-googlechart#readme"
