    "@angular/platform-browser-dynamic": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "2.0.2",
    "typings": "^1.3.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "zone.js": "^0.6.23"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  }
